# Local development environment variables
# Copy this file to .env.local and update with your actual values

# OpenAI API Key (required if not using Anthropic)
OPENAI_API_KEY=

# Anthropic API Key (required if not using OpenAI)
ANTHROPIC_API_KEY=xxxx

# n8n Webhook URL (required for sending Swagger to n8n)
N8N_WEBHOOK_URL=http://localhost:5678/webhook/swagger-receiver

# n8n Webhook Token (optional, for authentication)
N8N_WEBHOOK_TOKEN=

# CodeGen API Configuration
CODEGEN_API_KEY=your-codegen-api-key
CODEGEN_ORG_ID=your-codegen-org-id

# GitHub Access Token (optional, for automatic GitHub integration)
GITHUB_ACCESS_TOKEN=your-github-personal-access-token